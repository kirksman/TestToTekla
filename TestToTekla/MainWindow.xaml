        <Label x:Name="msg_IsConnect" Content="" HorizontalAlignment="Center" Height="30" VerticalAlignment="Top" Width="155" Margin="0,10,0,0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="16"/>

    </Grid>
